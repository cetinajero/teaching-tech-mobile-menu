<div data-role="page" id="sub{{ page | downcase | url_encode }}">
  {% assign title = site.data.pages[page].subpage.title %}

  {% include components/header.liquid
     title=title
     parent=page
  %}

  <div role="main" class="ui-content">
    <div>{{ site.data.pages[page].subpage.body }}</div>
  </div>

  {% include components/footer.liquid %}

</div>
